{
    "version": 2,
    "builds": [
        {
            "src": "ToDoList/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLamdbaSize": "15mb",
                "runtime": "python3"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ToDoList/wsgi.py"
        }
    ]
}
